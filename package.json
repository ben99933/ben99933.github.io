{
  "name": "ben99933.github.io",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "run-p type-check \"build-only {@}\" --",
    "test:unit": "vitest",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "build-only": "vite build",
    "dev": "vite  && npx tailwindcss -i ./src/assets/css/style.css -o ./src/assets/css/tailwind.css --minify",
    "format": "prettier --write src/",
    "preview": "vite preview",
    "type-check": "vue-tsc --build --force",
    "hotfix": "npx tailwindcss -i ./src/assets/css/style.css -o ./src/assets/css/tailwind.css --minify --watch",
    "dev:api": "nodemon",
    "build:api": "tsc -p tools/blogAdmin/tsconfig.json && tsc-alias -p tools/blogAdmin/tsconfig.json"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.6",
    "@highlightjs/vue-plugin": "^2.1.0",
    "autoprefixer": "^10",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "express": "^5.1.0",
    "github-markdown-css": "^5.8.1",
    "highlight.js": "^11.11.1",
    "highlightjs-line-numbers.js": "^2.9.0",
    "katex": "^0.16.21",
    "markdown-it": "^14.1.0",
    "markdown-it-attrs": "^4.3.1",
    "markdown-it-katex": "^2.0.3",
    "markdown-it-playground": "^0.0.5",
    "markdown-it-toc-done-right": "^4.2.0",
    "pinia": "^2.1.7",
    "postcss": "^8",
    "tailwindcss": "^3",
    "uuid": "^11.1.0",
    "vue": "^3.4.15",
    "vue-markdown": "^2.1.3",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsdom": "^21.1.6",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^20.11.10",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.0.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^24.0.0",
    "nodemon": "^3.1.10",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "sass-embedded": "^1.86.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.4",
    "typescript": "~5.3.0",
    "vite": "^7.1.2",
    "vitest": "^3.2.4",
    "vue-cli-plugin-tailwind": "~3.0.0",
    "vue-tsc": "^2.2.8"
  },
  "type": "commonjs"
}
